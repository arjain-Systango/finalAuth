{
  "name": "authentication",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18.18.2"
  },
  "scripts": {
    "build": "rm -rf build && tsc",
    "start": "node build/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' -r dotenv/config --exec 'ts-node --files' src/index.ts",
    "migration:create": "cd src/db/migration &&  typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/db/dbConnection",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/db/dbConnection",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "seed:run": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./src/db/dbConnection.ts",
    "seed:create": "cd src/db/seeder && ts-node ../../../node_modules/typeorm-extension/bin/cli.cjs seed:create --name "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.450.0",
    "@aws-sdk/client-ssm": "^3.450.0",
    "apple-signin-auth": "^1.7.5",
    "axios": "^1.6.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "joi-password": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "random-number-csprng": "^1.0.2",
    "randomstring": "^1.3.0",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.17",
    "typeorm-extension": "^3.1.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.16",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/qrcode": "^1.5.5",
    "@types/random-number-csprng": "^1.0.2",
    "@types/randomstring": "^1.1.11",
    "@types/speakeasy": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
